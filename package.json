    "version": "0.0.1",
    "publisher": "jtong",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.sayWhatYouSelected"
    ],
    "main": "./extension",
